<?eLayout xml="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/fs_video_fragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <cn.tee3.n2m.ui.fragment.T3VideoView
        android:layout_width="@dimen/width_30_80"
        android:layout_height="@dimen/height_20_80"
        android:layout_marginRight="@dimen/width_2_80"
        android:layout_marginTop="@dimen/height_5_80"
        android:id="@+id/video_up"
        android:tag="upperVideo"
        android:layout_gravity="right"/>
    <cn.tee3.n2m.ui.fragment.SelectVideoView
        android:layout_width="match_parent"
        android:layout_height="@dimen/height_60_80"
        android:layout_marginRight="@dimen/width_2_80"
        android:layout_marginTop="@dimen/height_5_80"
        android:visibility="gone"
        android:id="@+id/id_select_video"/>

    <cn.tee3.n2m.ui.fragment.T3VideoView
        android:id="@+id/video_bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:tag="backgroundVideo" />

</FrameLayout>